FILE NAME:-  hello_world_parallel.c

#include <omp.h>
#include <stdio.h>

int main (int argc, char* argv[])
{
    #pragma omp parallel
    {
        printf("Hello World... from thread = %d\n", omp_get_thread_num());
    }
    return 0;
}

COMP:-  gcc -fopenmp -o hello_world_parallel hello_world_parallel.c
EXEC:-  ./hello_world_parallel
O/P:-
Hello World... from thread = 0
Hello World... from thread = 1
Hello World... from thread = 2
Hello World... from thread = 3

!apt-get update
!apt-get install -y g++ # Install the GNU C++ compiler

GC:- %%writefile hello_world_parallel.c, !gcc -fopenmp -o hello_world_parallel hello_world_parallel.c, !./hello_world_parallel
